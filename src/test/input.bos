    Sub Initialize()
        Database.HeaderExpand.Table.RowInserted.Add(HeaderExpand_Inserted)
    End Sub

    Sub HeaderExpand_Inserted(sender As Object, args As RowChangedEventArgs)
        Dim hExRow As SalesQUHExpandRow = args.Row
